			"part '$1.g.dart';\n",
			"abstract class BuiltPost implements Built<BuiltPost, BuiltPostBuilder> {",
			"\tBuiltPost._();\n",
			"\tfactory BuiltPost([updates(BuiltPostBuilder b)]) = _\\$BuiltPost;\n",
			"\tstatic Serializer<BuiltPost> get serializer => _\\$builtPostSerializer;",
			"}"
		]
	},
